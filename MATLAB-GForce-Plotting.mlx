% Define your ThingSpeak channel and read API key
channelID = 2001772;
readAPIKey = '83M7ROH6IQ6QDXFF';

% Retrieve the data from ThingSpeak
data = thingSpeakRead(channelID, Fields=[4,5,6], NumPoints=6);

figure;
hold on;
x = data(:,1);
x = x / 1000;
y1 = data(:,2);
y2 = data(:,3);

plot(x,y1,'r');
plot(x,y2,'b');
xlabel('Time (seconds)');
ylabel('G-Force');
legend('Accel/Brake','Turn');
hold off;
